th: 28rem;\n    }\n\n    @include respond(phone){\n        width: 27.5rem;\n    }\n\n    &__header{\n        height: 50%;\n        color: white;\n        background-color: black;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n\n        &--avatar{\n            height: 10rem;\n            width: 10rem;\n            border-radius: 50%;\n        }\n    }\n\n    &__content{\n        height: 50%;\n        width: 80%;\n        margin: 0 auto;\n        display: flex;\n        flex-direction: column;\n        justify-content: center;\n        align-items: center;\n        font-size: 1.2rem;\n        font-weight: 400;\n        text-transform: uppercase;\n\n        & > *{\n            &:not(:last-child){\n                margin-bottom: .5rem;\n            }\n        }\n    }\n\n    &__name{\n        font-family: 'Barlow Semi Condensed', sans-serif;\n        font-size: 3rem;\n        font-weight: 700;\n        text-align: center;\n    }\n\n    &__email{\n        text-align: center;\n    }\n}"},"lineCount":null}},"error":null,"hash":"e24c5721cb20bf4cd37ac251b3842397","cacheData":{"env":{}}}