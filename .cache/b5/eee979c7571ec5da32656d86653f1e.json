ss":"@import '../../mixins/mixins';\n\n.sidebar{\n    background-color: white;\n    position: fixed;\n    top: 0;\n    right: 0;\n    height: 100vh;\n    width: 0%;\n    box-shadow: 0 .1rem 3rem rgba(black, .1);\n    text-transform: uppercase;\n    transition: all .5s ease;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-around;\n    z-index: 3;\n\n    & > *{\n        width: 80%;\n        margin: 0 auto;\n    }\n\n    &.active{\n        width: 25%;\n\n        @include respond(tab-land){\n            width: 35%;\n        }\n    \n        @include respond(phone){\n            width: 45%;\n        }\n    }\n\n    &.active &__close{\n        opacity: 1;\n        visibility: visible;\n        transition: all .5s ease;\n    }\n\n    &__close{\n        font-size: 6rem;\n        font-weight: 100;\n        font-family: 'Montserrat', sans-serif;\n        cursor: pointer;\n        opacity: 0;\n        visibility: hidden;\n    }\n\n    &__title{\n        font-size: 2rem;\n    }\n\n    &__heading{\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n    }\n\n    &__content{\n        font-size: 2rem;\n    }\n\n    &__list{\n        &--item{\n            list-style: none;\n            font-family: 'Montserrat', sans-serif;\n            font-size: 1.5rem;\n            text-transform: uppercase;\n            cursor: pointer;\n\n            &:not(:last-child){\n                margin: 1rem 0;\n            }\n        }\n    }\n}"},"lineCount":null}},"error":null,"hash":"8a10e3da1f2d3622fafaa5bd82b315b9","cacheData":{"env":{}}}